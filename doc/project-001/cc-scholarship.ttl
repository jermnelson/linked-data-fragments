    schema:pageStart "1088" ;
    schema:partOf <http://catalog.coloradocollege.edu/7045c627-4276-11e8-9d78-a41f7283f8d4> ;
    schema:url <https://doi.org/10.1130/B25249.1> ;
    cite:authorString "Henry C. Fricke." ;

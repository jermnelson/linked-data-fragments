    schema:url <http://0-search.ebscohost.com.tiger.coloradocollege.edu/login.aspx?direct=true&db=geh&AN=2008-115264> ;
    cite:authorString "Melissa A. Barton, Henry C. Fricke." ;
    cite:citationType "article" ;
